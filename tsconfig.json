{
  "compilerOptions": {
    "jsx": "react",
    "outDir": "./dummy_build",
    "target": "ES5",
    "lib": ["ES5", "DOM"],
    "checkJs": true,
    "allowJs": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
      "node_modules",
      "build"
  ],
  "include": [
    "src/**/*.js",
  ],
  "plugins": [
    "node_modules/jsdoc-typeof-plugin"
  ]
}